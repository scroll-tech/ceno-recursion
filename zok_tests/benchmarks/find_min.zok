def main(field low, field b, field c, field high) -> field:
    field[1200] arr = [0; 1200]
    field sum = 0
    for field i in 0..1200 do
        sum = sum + i
        arr[i] = sum
    endfor
    // random access to ensure array is written in memory
    field x = arr[b]

    field min = arr[low]
    field min_idx = low

    for field i in 0..1200 do
        if low < i && i < high && arr[i] < min then
            min = arr[i]
            min_idx = i
        endif
    endfor


    return min + (min_idx + x) * c
