def main(field high, field low, field b, field c) -> field:
    field[2048] arr = [0; 2048]
    field sum = 0
    for field i in 0..2048 do
        sum = sum + i
        arr[i] = sum
    endfor
    // random access to ensure array is written in memory
    field x = arr[b]

    field min = arr[low]
    field min_idx = low

    field i = low + 1
    while i < high do
        if arr[i] < min then
            min = arr[i]
            min_idx = i
        endif
        i = i + 1
    endwhile


    return min + (min_idx + x) * c
