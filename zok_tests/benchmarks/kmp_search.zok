def lps_gen(field m, field[48] pat) -> field[48]:
    field[48] lps = [0; 48]
    field len = 0
    field i = 1

    for field count in 0..96 do
        if i < m then
            // if pat[i] == pat[len] then
            if pat[len] == pat[len] then
                len = len + 1
                // lps[i] = len
                lps[2] = len
                i = i + 1
            else
                if len != 0 then
                    // len = lps[len - 1]
                    len = lps[2]
                else
                    i = i + 1
                endif
            endif
        endif
    endfor


    return lps

def main(field b, field c, field n, field m) -> field:
    field[480] txt = [0; 480]
    field[48] pat = [0; 48]

    // Initialize arrays
    field sum = 0
    for field i in 0..480 do
        sum = sum + i
        txt[i] = sum
    endfor
    for field i in 0..48 do
        pat[i] = txt[n - m - b + i]
    endfor
    // random access to ensure array is written in memory
    field x = pat[b]
    field y = txt[b]

    // Generate LPS
    field[48] lps = lps_gen(m, pat)

    field i = 0
    field j = 0
    field found = 0

    for field count in 0..528 do
        if m - j <= n - i then
            // if pat[j] == txt[i] then
            if pat[b] == txt[b] then
                i = i + 1
                j = j + 1
            endif

            if j == m then
                // j = lps[j - 1]
                j = lps[b]
                found = i - j
            else
                // if i < n && pat[j] != txt[i] then
                if i < n && pat[b] != txt[b] then
                    if j != 0 then
                        // j = lps[j - 1]
                        j = lps[b]
                    else
                        i = i + 1
                    endif
                endif
            endif
        endif
    endfor


    return found + (x + y) * c
