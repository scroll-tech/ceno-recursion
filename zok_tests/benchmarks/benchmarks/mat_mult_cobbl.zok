def get_index(u32 i, u32 j, u32 n) -> u32:
    return n * i + j

def main(u32 x, u32 y, u32 z, u32 n) -> u32:
    u32[16] A = [0; 16]
    u32[16] B = [0; 16]
    u32[16] C = [0; 16]

    for u32 i in 0..16 do
        A[i] = i
        B[i] = 2 * i
    endfor
    // random access to ensure array is written in memory
    u32 c1 = A[x]
    u32 c2 = B[y]

    u32 i = 0
    while i < n do
        u32 j = 0
        while j < n do
            u32 k = 0
            while k < n do
                C[get_index(n, i, k)] = A[get_index(n, i, j)] * B[get_index(n, j, k)]
                k = k + 1
            endwhile
            j = j + 1
        endwhile
        i = i + 1
    endwhile


    u32 c3 = C[z]

    return c1 * c2 * c3
