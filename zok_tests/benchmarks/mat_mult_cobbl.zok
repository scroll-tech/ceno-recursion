def get_index(field n, field i, field j) -> field:
    return n * i + j

def main(field n, field x, field y, field z) -> field:
    field[4] A = [0; 4]
    field[4] B = [0; 4]
    field[4] C = [0; 4]

    for field i in 0..4 do
        A[i] = i
        B[i] = 2 * i
    endfor
    // random access to ensure array is written in memory
    field c1 = A[x]
    field c2 = B[y]

    field i = 0
    while i < n do
        field j = 0
        while j < n do
            field k = 0
            while k < n do
                C[get_index(n, i, k)] = A[get_index(n, i, j)] * B[get_index(n, j, k)]
                k = k + 1
            endwhile
            j = j + 1
        endwhile
        i = i + 1
    endwhile


    field c3 = C[z]

    return c1 * c2 * c3
