def get_index(u32 i, u32 j, u32 n) -> u32:
    return n * i + j

def main(u32 x, u32 y, u32 z, u32 n) -> u32:
    u32[36] A = [0; 36]
    u32[36] B = [0; 36]
    u32[36] C = [0; 36]

    for u32 i in 0..36 do
        A[i] = i
        B[i] = 2 * i
    endfor
    // random access to ensure array is written in memory
    u32 c1 = A[x]
    u32 c2 = B[y]

    for u32 i in 0..6 do
        if i < n then
            for u32 j in 0..6 do
                if j < n then
                    for u32 k in 0..6 do
                        if k < n then
                            C[get_index(n, i, k)] = A[get_index(n, i, j)] * B[get_index(n, j, k)]
                        endif
                    endfor
                endif
            endfor
        endif
    endfor


    u32 c3 = C[z]

    return c1 * c2 * c3
