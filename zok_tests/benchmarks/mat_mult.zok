def get_index(field n, field i, field j) -> field:
    return n * i + j

def main(field n, field x, field y, field z) -> field:
    field[64] A = [0; 64]
    field[64] B = [0; 64]
    field[64] C = [0; 64]

    for field i in 0..64 do
        A[i] = i
        B[i] = 2 * i
    endfor
    // random access to ensure array is written in memory
    field c1 = A[x]
    field c2 = B[y]

    for field i in 0..8 do
        if i < n then
            for field j in 0..8 do
                if j < n then
                    for field k in 0..8 do
                        if k < n then
                            C[get_index(n, i, k)] = A[get_index(n, i, j)] * B[get_index(n, j, k)]
                        endif
                    endfor
                endif
            endfor
        endif
    endfor


    field c3 = C[z]

    return c1 * c2 * c3
