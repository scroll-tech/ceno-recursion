CONST:
max_n 2

INPUT:
x 0
y 0
z 0

PROGRAM:
def get_index(u32 i, u32 j, u32 n) -> u32:
    return n * i + j

def main(u32 x, u32 y, u32 z, u32 n) -> u32:
    u32[{max_n * max_n}] A = [0; {max_n * max_n}]
    u32[{max_n * max_n}] B = [0; {max_n * max_n}]
    u32[{max_n * max_n}] C = [0; {max_n * max_n}]

    for u32 i in 0..{max_n * max_n} do
        A[i] = i
        B[i] = 2 * i
    endfor
    // random access to ensure array is written in memory
    u32 c1 = A[x]
    u32 c2 = B[y]

    // BASELINE
    for u32 i in 0..{max_n} do
        if i < n then
            for u32 j in 0..{max_n} do
                if j < n then
                    for u32 k in 0..{max_n} do
                        if k < n then
                            C[get_index(n, i, k)] = A[get_index(n, i, j)] * B[get_index(n, j, k)]
                        endif
                    endfor
                endif
            endfor
        endif
    endfor

    // COBBL
    u32 i = 0
    while i < n do
        u32 j = 0
        while j < n do
            u32 k = 0
            while k < n do
                C[get_index(n, i, k)] = A[get_index(n, i, j)] * B[get_index(n, j, k)]
                k = k + 1
            endwhile
            j = j + 1
        endwhile
        i = i + 1
    endwhile

    // END_SPLIT

    u32 c3 = C[z]

    return c1 * c2 * c3
