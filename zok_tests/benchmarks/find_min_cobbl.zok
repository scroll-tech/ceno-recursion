def main(u32 low, u32 b, u32 c, u32 high) -> u32:
    u32[high] arr = []
    u32 sum = 0
    for u32 i in 0..high do
        sum = sum + i
        arr[i] = sum
    endfor
    // random access to ensure array is written in memory
    u32 x = arr[b]

    u32 min = arr[low]
    u32 min_idx = low

    for u32 i in low + 1..high do
        if arr[i] < min then
            min = arr[i]
            min_idx = i
        endif
    endfor


    return min + (min_idx + x) * c
